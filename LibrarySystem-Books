public class LibrarySystem-Books {
    public static void main(String[] args) {
        
        Author author = new Author("Suzanne Collins", "suzanne.collins@gmail.com", "American");

        Book book = new Book("The Ballad of Songbirds and Snakes (A Hunger Games Novel)", $19.59, author);

        System.out.println(book.toString());
    }
}
